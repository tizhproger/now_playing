{
    "name": "Now Playing - OBS",
    "version": "1.2.3",
    "description": "Client for OBS now playing widget",
    "manifest_version": 3,
    "author": "Tizhproger inspired by Tuna",
    "content_scripts": [
        {
        "matches": ["https://*.youtube.com/*", "https://*.spotify.com/*", "https://*.soundcloud.com/*"],
        "js": ["nowplaying_client.js"]
        }
      ],
	"icons": {
	  "16": "16.png",
	  "32": "32.png",
	  "48": "48.png",
	  "64": "64.png",
	  "128": "128.png"
	}
}
